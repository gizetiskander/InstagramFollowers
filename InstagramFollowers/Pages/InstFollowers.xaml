        xmlns:local="clr-namespace:InstagramFollowers.Pages"
        mc:Ignorable="d"
        Title="InstFollowers" Height="800" Width="1440" Background="Black">
    <Grid>
        <Image HorizontalAlignment="Left" Height="82" Margin="1284,10,0,0" VerticalAlignment="Top" Width="82" Source="inst_icon.png"/>
